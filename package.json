    "imap": "^0.8.19",
    "mailparser": "^3.6.5",
    "sharp": "^0.32.6",
    "tesseract.js": "^5.0.2",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "repository": {
